;homeUOL.register("widgetTailtarget",["jquery","basepath","Mustache","clickuol","ga"],function widgetFutebol($,basepath,Mustache,clickuol,GA){var servico=basepath("/camaleao/api/tt-widget-noticias.json?"+(window.cacheId||"v1.0"));var tagInterests={"educacao":{"interest":"55","tag":"16614","chapeu":"Educação"},"carros":{"interest":"CA6824","tag":"13014","chapeu":"Carros"},"tecnologia":{"interest":"CA7239","tag":"13703","chapeu":"Tilt"},"economia":{"interest":"CA7248","tag":"22373","chapeu":"Economia"},"vivabem":{"interest":"13","tag":"77387","chapeu":"Saúde - Vivabem"}};var $module,$container,markup;var priority=[];var userInterest=[];var exceptTags=[];var intLabels={};var tagLabels=[];for(var item in tagInterests){if(tagInterests.hasOwnProperty(item)){var interest=tagInterests[item].interest;var tagId=tagInterests[item].tag;priority.push(tagId);tagLabels.push(item);intLabels[tagId]=tagInterests[item];if(TTUser&&TTUser.hasInterest(interest)==true){userInterest.push(tagId);}else{exceptTags.push(tagId);}}}var widgetItems=3;var limitChars=70;var newsBl=[];var interestedNews=function(docs){var findNews=function(tag){return docs[tag]&&docs[tag].length&&docs[tag].shift();};var findInArr=function(arr){while(news.length<widgetItems&&arr.length){for(var i=0;i<arr.length;i++){if(news.length>=widgetItems){break;}var tag=arr[i];var headline=findNews(tag);var blacklistVerify=function(){if(headline&&newsBl.indexOf(headline.id)!==-1){headline=findNews(tag);blacklistVerify();}};blacklistVerify();if(headline){headline.index=news.length;headline.indexPlus=headline.index+1;headline.tagLabel=tagLabels[priority.indexOf(tag)];headline.chapeu=intLabels[tag].chapeu||"";headline.hasInterest=false;if(userInterest.indexOf(tag)!=-1){headline.hasInterest=true;}if(headline.titulo.length>limitChars&&headline.imagem){headline.titulo=headline.titulo.slice(0,limitChars-1)+"…";}news.push(headline);newsBl.push(headline.id);}else{arr.splice(i,1);i--;}}}};var orderToLook=[userInterest,exceptTags];var new