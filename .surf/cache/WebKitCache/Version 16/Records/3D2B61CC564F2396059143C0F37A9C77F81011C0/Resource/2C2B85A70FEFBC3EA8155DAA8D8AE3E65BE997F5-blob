y(_this,[event]);}};function open(event){var cfg=event.data;var $submenu=cfg.$subEl;closeAllSub();clearTimeout(timeoutClose);cfg.$canalEl.addClass("show");if(opacityTransition){$submenu.addClass("show").stop().fadeIn(cfg.isTouch===false?fadeTime:0);opacityTransition=false;}else{$submenu.addClass("show").show();}calcSubmenuPosition(cfg.canalName,$submenu,cfg.$canalEl);window.echo&&echo.throttle();}var closeEvent=function(event){var _this=this;clearTimeout(timeoutClose);timeoutClose=setTimeout(function(){close.apply(_this,[event]);},event.data.isTouch===false?timeToClose:20);};function close(event){var cfg=event.data;clearTimeout(timeoutClose);cfg.$canalEl.removeClass("show");cfg.$canalEl.parent().removeClass("hover");cfg.$subEl.removeClass("show").fadeOut(200);opacityTransition=true;firstHover=true;}function submenuMouseEvents(event){switch(normalizeEventType(event.type)){case"mouseenter":clearTimeout(timeoutClose);break;case"mouseleave":$(this).closest("li").find(".HU_menu__link").trigger("mouseleave").trigger("pointerout");break;}}function stopEvent(event){event.preventDefault();event.stopPropagation();}function getMenuObject(originalEvent){var channelName=this.getAttribute("data-menu");var $subEl=$submenus.filter('[data-menu="'+channelName+'"]');var $item=$(this).closest("li");var eventType=originalEvent.type;var isTouch=isTouchEvent(originalEvent);eventType=eventType.replace("mousedown","touchend");return{type:eventType,isTouch:isTouch,canalName:channelName,canalEl:this,$canalEl:$(this),$item:$item,$subEl:$subEl};}function hoverItem(event,toggle){if(toggle===true){event.data.$item.toggleClass("hover");}else{$canais.removeClass("hover");event.data.$item.addClass("hover");}}function canalMouseEvents(event){event.stopPropagation();event.data=getMenuObject.call(this,event);var $this=$(this);var type=event.data.type;var $subEl=event.data.$subEl;var isTouch=isTouchEvent(event);var noSub=$subEl.size()===0;if((isTouch===false&&type==="click")||noSub===true){hoverItem(event,noSub);return true;}hoverItem(event);if(isTouch===true&&(type==="click"||type.match(/pointerover|pointerout|mouseenter/)!==null)){stopEvent(event);return false;}event.preventDefault();clearTimeout(timeoutOpen);if(isTouchEvent(event)===true){event.data.isTouch=true;if($subEl.size()>0){event.data.type=$this.hasClass("show")===false?"mouseenter":"mouseleave";}}switch(normalizeEventType(event.data.type)){case"mouseenter":openEvent.call(this,event);break;case"mouseleave":closeEvent.call(this,event);break;}return true;}function calcSubmenuPosition(menu,$submenu,$link){if(calculatedPositions[menu]){return false;}var itemOffsetLeft=$link.offset().left;var submenuEmptySpaceBefore=itemOffsetLeft-getMenuOffsetLeft();var submenuWidth=$submenu.width();var left=(submenuEmptySpaceBefore)+submenuWidth<1180?submenuEmptySpaceBefore:-1;var css={};if(left>=0){css.left=left-5;css.right="auto";}else{css.left="auto";css.right="-4px";}$submenu.css(css);configBlocksHeight($submenu);calculatedPositions[menu]=true;}function configBlocksHeight($submenu){var $contentBlocks=$submenu.find(".HU_submenu__col");$contentBlocks.height(findBlocksMaxHeight($contentBlocks));}function findBlocksMaxHeight($contentBlocks){var maxHeight=0;$contentBlocks.each(function(index,el){maxHeight=Math.max(maxHeight,$(el).height());});return maxHeight;}$canaisLinks.on(pointerEvents,canalMouseEvents);$submenus.on("mouseenter mouseleave",submenuMouseEvents);$window.on("resize",setMenuOffsetLeft);$body.on("click",closeAllSub);setMenuOffsetLeft();};});
