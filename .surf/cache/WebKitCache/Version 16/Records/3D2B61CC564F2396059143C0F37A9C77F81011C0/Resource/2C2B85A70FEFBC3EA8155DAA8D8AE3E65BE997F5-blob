;homeUOL.register("busca",["jquery","metrics-ga"],function($,metricsGA){var formInputsType=['input[type="text"]','input[type="password"]','input[type="radio"]','input[type="checkbox"]','input[type="select"]'].join(",");var rules={"formBuscaCursosOnline":trataCursosOnline,"formBuscaEmpregoCerto":trataEmpregoCerto,"formBuscaShopping":trataShopping,"formBuscaRadioUOL":trataRadioUOL,"formBuscaUOLJogos":trataUOLJogos,"formBuscaEducacao":trataEducacao,"formBuscaVideos":trataVideos,"formBusca":trataBuscaTopo,"formBuscaMeuNegocio":trataBuscaMeuNegocio};function input(form,name,value){form=$(form);form.prepend('<input type="hidden" name="'+name+'" value="'+value+'" />');}function trataBuscaTopo($inputs,data){var $form=$(this);var $input=$inputs.filter('input[name="q"]');var query=$input.val();query=encodeURIComponent(query);if(query===""){return sendToUrl(data.emptyUrl);}var type="";type=/\#/.test(this.action)?this.action.replace(/.*\#/,"").toUpperCase():type;type=/^https:\/\/venda\.host\.uol/.test(this.action)?"DOMINIO":type;this.action=this.action.replace(/\#.*/,"");var useInputQ=false;switch(type){case"UOL":input(this,"type","all");input(this,"searchon","uol");input(this,"term",query);break;case"IMAGEM":input(this,"type","images");input(this,"searchon","all");input(this,"term",query);break;case"DICIONARIO":input(this,"redirect","dicionario");useInputQ=true;break;case"DOMINIO":query=validDomain($input.val());input(this,"extension",".com.br");input(this,"searchType","simples");input(this,"domain1",query);break;default:input(this,"term",query);}var queryString="?";$form.find("input").each(function(){if(this.name==="q"&&!useInputQ){return;}queryString+=this.name+"="+decodeURIComponent(this.value)+"&";});queryString=queryString.substring(0,queryString.length-1);hit(this.action+queryString,query,this);return sendToUrl(this.action+queryString);}function trataRadioUOL($inputs,data){var $input=$inputs.filter('input[name="q"]');var query=$input.val();hit(query,this);if(query===""){