;homeUOL.register("refresh",["debugerConsole"],function(DConsole){if(!Date.now){Date.now=function now(){return new Date().getTime();};}var userAgent=navigator.userAgent.toLowerCase();var timeToReload=parseInt(TIME_TO_RELOAD,10)||250001;var startTime=Date.now();var pausedQueue=[];var timer=null;function isPaused(){return !!pausedQueue.length;}function hasStarted(){return timer===null;}function clearTimer(){clearTimeout(timer);timer=null;}function addToQueue(key){if(pausedQueue.indexOf(key)==-1){pausedQueue.push(key);}}function removeFromQueue(key){if(pausedQueue.indexOf(key)>=0){pausedQueue.splice(pausedQueue.indexOf(key),1);}}function reload(){if(!isPaused()){userAgent.match(/msie|rv:/)?window.history.go(0):window.location.reload();}}function pause(moduleName){if(moduleName){DConsole.warn("[refresh] Pause "+moduleName);if(!isPaused()){timeToReload-=Date.now()-startTime;}addToQueue(moduleName);DConsole.warn("[refresh] Lista de pause",pausedQueue);clearTimer();}}function start(moduleName){if(moduleName){DConsole.warn("[refresh] Start "+moduleName);removeFromQueue(moduleName);DConsole.warn("[refresh] Lista de pause",pausedQueue);if(!isPaused()&&hasStarted()){startTime=Date.now();timeToReload=getTimeToReload();DConsole.warn("[refresh] Tempo até reload",timeToReload);timer=setTimeout(reload,timeToReload);}}}function getTimeToReload(){return timeToReload<10000?10000:timeToReload;}return{pause:pause,start:start};});
