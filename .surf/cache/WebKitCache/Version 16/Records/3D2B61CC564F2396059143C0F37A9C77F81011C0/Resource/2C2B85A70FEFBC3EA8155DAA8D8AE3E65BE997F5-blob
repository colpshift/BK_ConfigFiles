;homeUOL.register("hibridHighlights",["metrics-ga","Mustache","editorialLinks","ga","basepath"],function(metricsGA,Mustache,EditorialLinks,GA,BasePath){var $module,$template,$container;var requestedHighlights=[];var highlightGridItems=[];var displayedRows=1;var listContainer;var showMoreContainer;var rowElements;var postionModule;var clampClass="destaque-row-truncated";var SERVICE="/camaleao/api/destaques-24hs.json";var labeled={today:false,yesterday:false};var mockDate=location.search.split("destaque-date=")[1];var ISmockJson=location.search.split("mockDestaque24hs=")[1];function init(){listContainer=$module[0].querySelector(".destaque-lista");showMoreContainer=$module[0].querySelector(".destaque-see-more");doRequest();bindSeeMoreButton();getPostionModule();}function bindErrorButton(){const errorButton=$module[0].querySelector(".destaque-error-button");errorButton&&errorButton.addEventListener("click",function(){metricsGA({action:"clique",category:"bloco-editorial-topo-"+postionModule,area:"topo",label:"tentar-novamente",position:"chamada-destaque-24h"});const highlightRow=$module[0].querySelector(".destaque-row");highlightRow&&highlightRow.classList.add("destaque-hidden");isLoading(true);setTimeout(function(){highlightRow&&highlightRow.remove();doRequest();},800);});}function doRequest(){var linkservice=SERVICE;if(ISmockJson){linkservice+="?mockDestaque24hs=true";}else{if(mockDate){linkservice+="?destaque-date="+mockDate;}}linkservice=BasePath(linkservice);$.ajax({url:linkservice}).done(filterData).fail(function(){renderErrorScreen();});}function renderErrorScreen(){isLoading(false);$container.html(Mustache.render($template,{errorScreen:true}));bindErrorButton();}function bindSeeMoreButton(){var seeMoreButton=$module[0].querySelector(".destaque-see-more-button");seeMoreButton&&seeMoreButton.addEventListener("click",function(){showMore();});}function getPostionModule(){var thisModuleParent=$module[0]&&$module[0].parentElement;postionModule=thisModuleParent.className.indexOf("topo-hibrido-rodape")>-1?2:1;}function bindMetrics(){var highlightLinks=$module[0]&&$module[0].querySelectorAll(".destaque-item");highlightLinks&&GA(highlightLinks);}function showMore(){var firstRowTruncated=document.querySelector("."+clampClass);if(firstRowTruncated){firstRowTruncated.classList.remove(clampClass);Array.prototype.forEach.call(firstRowTruncated.querySelectorAll(".clamped-text"),function(text){Clamp.remove(text);});}metricsGA({action:"clique",category:"bloco-editorial-topo-"+postionModule,area:"topo",label:"ver-mais",position:"chamada-destaque-24h"});const rowsToShow=muchRowsToShow();if(rowsToShow){displayedRows+=rowsToShow;updateListHeight();shouldHaveMoreStyle(!!muchRowsToShow());}}function isLoading(isLoading){if(isLoading){$module[0].classList.remove("loading_hidden");}else{$module[0].classList.add("loading_hidden");}}function removeLoadingState(){isLoading(false);setTimeout(function(){$module[0].classList.remove("loading","loading_withFade","loading_hidden");},200);}function getDayTimeStamp(){if(!!mockDate){return mockDate.substring(0,8);}var date=new Date();return date.getFullYear()+("0"+(date.getMonth()+1)).slice(-2)+("0"+(date.getDate())).slice(-2);}function filterData(response){if((response.docs||[]).length){response.docs.forEach(function(doc){if(!EditorialLinks.exists(doc["url-uol"])){var highlight=highlightFactory(doc);if(highlight.time!==""){requestedHighlights.push(highlight);}}});}requestedHighlights.length&&requestedHighlights.length>2?showRequestedHighlights():renderErrorScreen();}function decodeHtml(html){let el=document.createElement("textarea");el.innerHTML=html;const txt=el.value;el=null;return txt;}function highlightFactory(doc){var item={kicker:doc.chapeu,title:decodeHtml(doc.titulo),time:getTime(doc.created),link:doc["url-uol"],hasImage:false};if(doc.imagem){item.hasImage=true;item.image={src:doc.imagem,credits:doc["imagem-credito"]};}return item;}function getTime(timestamp){var now=Date.now();var year=timestamp.substring(0,4);var month=timestamp.substring(4,6)-1;var day=timestamp.substring(6,8);var hour=timestamp.substring(8,10);var minutes=timestamp.substring(10,12);var seconds=timestamp.substring(12,14);var date=new Date(year,month,day,hour,minutes,seconds).getTime();var time=Math.floor((now-date)/3600000);var lastHour=time<=1;var textHour="há "+(lastHour?1:time)+" hora"+(lastHour?"":"s");if(time>24){textHour="";}return textHour;}function renderRow(rowData){return Mustache.render($template,rowData);}function showRequestedHighlights(){removeLoadingState();var markup="";var lines=0;for(var rowNumber=0;requestedHighlights.length&&rowNumber<100;rowNumber++){const row=requestedHighlights.splice(0,4);if(row.length>=3){highlightGridItems[rowNumber]=row;markup+=renderRow({items:highlightGridItems[rowNumber],firstRow:lines===0});}lines++;}appendMarkup(markup);}function updateListHeight(){if(!rowElements){rowElements=$module[0].querySelectorAll(".destaque-row");}var newHeight=0;if(rowElements&&muchRowsToShow()){newHeight=rowElements[displayedRows].offsetTop+4;}else{newHeight=$container[0].offsetHeight;}requestAnimationFrame(function(){listContainer.style.maxHeight=newHeight+"px";});}function shouldHaveMoreStyle(shouldShow){if(shouldShow){if(!showMoreContainer.classList.contains("destaque-see-more_visible")){showMoreContainer.classList.add("destaque-see-more_visible");}}else{showMoreContainer.classList.remove("destaque-see-more_visible");listContainer.classList.remove("destaque-lista_withMore");setTimeout(function(){showMoreContainer.querySelector(".destaque-see-more-button").remove();},810);}}function muchRowsToShow(){const diffRows=highlightGridItems.length-displayedRows;return diffRows>2?2:diffRows;}function appendMarkup(markup){$container.append(markup);bindMetrics();updateListHeight();shouldHaveMoreStyle(!!muchRowsToShow());echo.addImages($module);var truncatedTitles=document.querySelectorAll("."+clampClass+" .destaque-item-title");if(window.Clamp&&truncatedTitles.length){Array.pro