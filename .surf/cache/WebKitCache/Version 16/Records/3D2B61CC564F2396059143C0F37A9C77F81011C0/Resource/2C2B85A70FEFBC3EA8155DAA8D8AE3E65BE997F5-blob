;homeUOL.register("placarFutebol",["jquery","usocket","cssAnimSupport"],function($,usocket,cssAnimSupport){var WS_PREFIX="home-placar-futebol-";if(location.search.match(/qa/)){WS_PREFIX="qa-"+WS_PREFIX;}var games={};function isPenaltis(status){return status.penaltis==="2"||status.penaltis==="3";}function isStarted(status){return status.partida==="2"||status.partida==="3";}function itsOver(status){return status.partida==="3";}function hasTwoDigits(game){var action=(parseInt(game.data.equipes.e1["saldo-gols"],10)>9||parseInt(game.data.equipes.e2["saldo-gols"],10)>9)?"add":"remove";game.$game[action+"Class"]("has-two-digits");}function updateMatchStatus(game,status){var classes=["jogo-encerrado","jogo-aovivo","jogo-pre"];game.$game.removeClass(classes.join(" "));if(itsOver(status)==true){return game.$game.addClass("jogo-encerrado");}if(isStarted(status)==true){return game.$game.addClass("jogo-aovivo");}return game.$game.addClass("jogo-pre");}function init(module){var $jogos=module.find(".jogos").find(".jogo");$jogos.each(registerGame);module.on("mouseenter touchend",".item-carrossel",changeGame(module));}function changeGame(module){var $carrosselGames=module.find(".item-carrossel");return function onMouseOverGames(){$carrosselGames.removeClass("selected");$(this).addClass("selected");var game=getGameDom(this,this.getAttribute("data-channel"));game.$games.removeClass("selected");game.$game.addClass("selected");};}function getGameDom(el,id){return $(el).closest('[data-module="placarFutebol"]').find(".jogo[data-channel="+id+"]").data("placar");}function registerGame(){var $this=$(this),$module=$this.closest('[data-module="placarFutebol"]'),channelName=WS_PREFIX+$this.data("channel");var modData={$module:$module,$link:$module.find("a"),$games:$module.find(".jogos .jogo"),$game:$this,update:{"saldo-gols":$this.find(".saldo-gols"),"saldo-penaltis":$this.find(".saldo-penaltis")},data:undefined};games[channelName]=games[channelName]||[];games[channelName].push(modData);$this.data("placar",modData);usocket.connect(channelName).on("message",message);}function message(message){var channelName=message.channelName;if(channelName===undefined){return false;}for(var g=0;g<games[message.channelName].length;g++){var game=games[message.channelName][g],penaltis=isPenaltis(message.status);delete game.lastGoal;if(JSON.stringify(game.data)===JSON.stringify(message)){return false;}updateMatchStatus(game,message.status);var action=game.$module.find(".jogo-pre")?"add":"remove";game.$module[action+"Class"]("tem-pre-jogo");if(isStarted(message.status)===false){return false;}if(game.data!==undefined&&JSON.stringify(game.data.equipes)!==JSON.stringify(message.equipes)){var time1,time2;if(penaltis===false){time1=game.data.equipes.e1["saldo-gols"]!==message.equipes.e1["saldo-gols"];time2=game.data.equipes.e2["saldo-gols"]!==message.equipes.e2["saldo-gols"];}else{time1=game.data.equipes.e1["saldo-penaltis"]!==message.equipes.e1["saldo-penaltis"];time2=game.data.equipes.e2["saldo-penaltis"]!==message.equipes.e2["saldo-penaltis"];}game.data=message;game.lastGoal=time1===true?"e1":"e2";animate(game,update);}else{game.data=message;update(game);}hasTwoDigits(game);updateLink(game);if(penaltis===true){game.$game.addClass("penaltis");updatePenaltis(game);}else{game.$game.removeClass("penaltis");}}}function update(game){var saldoGols=game.data.equipes,$saldoGols=game.update["saldo-gols"];$saldoGols.eq(0).html(saldoGols.e1["saldo-gols"]);$saldoGols.eq(1).html(saldoGols.e2["saldo-gols"]);}function updatePenaltis(game){var saldoPenaltis=game.data.equipes,$saldoPenaltis=game.update["saldo-penaltis"];$saldoPenaltis.eq(0).html(saldoPenaltis.e1["saldo-penaltis"]);$saldoPenaltis.eq(1).html(saldoPenaltis.e2["saldo-penaltis"]);}function updateLink(game){if(game.$link){if(game.data["url-pos-jogo"]&&itsOver(game.data.status)){game.$game.find("a").attr("href",game.data["url-pos-jogo"]);game.$game.find(".label-pos:not(.pos-editorial)").text("Pós-Jogo");}else{if(game.data.url){game.$game.find("a").attr("href",game.data.url);game.$game.find(".label-pos:not(.pos-editorial)").text("Encerrado");}}}}function animate(game,then){var equipes=game.data.equipes,eGol=equipes[game.lastGoal],animSupport=cssAnimSupport()===false?"static-anim ":"";var animGolEl=game.$module.find(".anim-gol");if(animGolEl.size()>0){game.$module.find(".anim-gol").addClass(animSupport+"anim");setTimeout(showBrasao(eGol,game,then),4000);}else{then(game);}}function showBrasao(eGol,game,then){return function(){var $animContainer=game.$module.find(".anim-gol"),brasao=game.$game.find(".brasao."+game.lastGoal).attr("src").replace(/(\d{2}x\d{2})/,"40x40");$animContainer.find(".brasao").html('<img src="'+brasao+'" width="40" height="40" />');$animContainer.addClass("anim-brasao");setTimeout(function(){$animContainer.removeClass("anim anim-brasao static-anim");$animContainer.find(".brasao").empty();then(game);},4500);};}return init;});
