;window["_cbv_strategies"]=window["_cbv_strategies"]||[];window["_cbv_strategies"].push(HtmlVideoStrategy);function HtmlVideoStrategy(player){this.player_=player;this.ready_=false;this.currentAdPosition_=undefined;this.videoPlayed_=false;this.viewStartTime_=new Date().getTime();this.videoStartTime_=undefined;this.subscribeEvents_();}HtmlVideoStrategy.ContentType={AD:"ad",CONTENT:"ct"};HtmlVideoStrategy.AdPosition={PREROLL:"a1",MIDROLL:"a2",POSTROLL:"a3",OVERLAY:"a4",SPECIAL:"a5"};HtmlVideoStrategy.VideoState={UNPLAYED:"s1",PLAYED:"s2",STOPPED:"s3",COMPLETED:"s4"};HtmlVideoStrategy.prototype.subscribeEvents_=function(){if(this.player_.readyState>2){this.onPlaybackReady_();}if(this.player_.currentTime>0&&!this.player_.paused&&!this.player_.ended){this.onVideoPlay_();}this.player_.addEventListener("canplay",this.bind_(this.onPlaybackReady_,this));this.player_.addEventListener("playing",this.bind_(this.onVideoPlay_,this));};HtmlVideoStrategy.prototype.bind_=function(fn,context){return function(){fn.call(context);};};HtmlVideoStrategy.prototype.onPlaybackReady_=function(){this.ready_=true;};HtmlVideoStrategy.prototype.onVideoPlay_=function(){this.videoStartTime_=new Date().getTime();this.videoPlayed_=true;};HtmlVideoStrategy.prototype.isReady=function(){return this.ready_;};HtmlVideoStrategy.prototype.getTitle=function(){return(this.player_.attributes["title"]&&this.player_.attributes["title"].value)||"";};HtmlVideoStrategy.prototype.getVideoPath=function(){var mediaID=null;try{mediaID=window.playerMOV&&playerMOV.mediaId||this.player_.parentNode.parentNode.parentNode.getAttribute("data-mediaid");}catch(err){console.log(err,"Erro ao obter media ID do player UOL MAIS - cbv strategy");}return mediaID||"";};HtmlVideoStrategy.prototype.getContentType=function(){if(window.playerMOV&&playerMOV.isPlayingAd()){return HtmlVideoStrategy.ContentType.AD;}return HtmlVideoStrategy.ContentType.CONTENT;};HtmlVideoStrategy.prototype.getAdPosition=function(){return"";};HtmlVideoStrategy.prototype.getTotalDuration=function(){var d=this.player_["duration"];return this.getTimeInSeconds_(d);};HtmlVideoStrategy.prototype.getState=function(){if(!this.videoPlayed_){return HtmlVideoStrategy.VideoState.UNPLAYED;}if(this.player_.ended){return HtmlVideoStrategy.VideoState.COMPLETED;}if(this.player_.paused){return HtmlVideoStrategy.VideoState.STOPPED;}return HtmlVideoStrategy.VideoState.PLAYED;};HtmlVideoStrategy.prototype.timeElapsed_=function(timestamp){if(timestamp===undefined){return 0;}var t=new Date().getTime();return t-timestamp;};HtmlVideoStrategy.prototype.onEmbedCodeChanged_=function(){this.currentAdPosition_=undefined;this.adStartTime_=undefined;};HtmlVideoStrategy.prototype.getCurrentPlayTime=function(){var pt=this.player_["currentTime"];return this.getTimeInSeconds_(pt);};HtmlVideoStrategy.prototype.getBitrate=function(){return -1;};HtmlVideoStrategy.prototype.getThumbnailPath=function(){return(this.player_.attributes["poster"]&&this.player_.attributes["poster"].value)||"";};HtmlVideoStrategy.prototype.getPlayerType=function(){};HtmlVideoStrategy.prototype.getViewStartTime=function(){if(isNaN(this.viewStartTime_)){return 0;}return this.timeElapsed_(this.viewStartTime_);};HtmlVideoStrategy.prototype.getViewPlayTime=function(){if(this.videoPlayed_){return this.timeElapsed_(this.viewStartTime_);}return -1;};HtmlVideoStrategy.prototype.getViewAdPlayTime=function(){return -1;};HtmlVideoStrategy.prototype.getTimeInSeconds_=function(t){return((t===-1||isNaN(t))?-1:t*1000);};HtmlVideoStrategy.verify=function(player){return player instanceof HTMLElement&&player.nodeName==="VIDEO";};
