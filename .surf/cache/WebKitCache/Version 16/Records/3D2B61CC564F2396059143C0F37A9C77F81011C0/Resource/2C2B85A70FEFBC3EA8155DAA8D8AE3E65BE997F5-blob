;homeUOL.register("horizontalBBB",["jquery","storage","metrics-ga","debugerConsole"],function($,storage,metricsGA,DConsole){var $body=$("body");var modulo;function modVotacaoBBB(){var modvotacao=modulo.find(".horizontal-votacao");if(modvotacao.length!=0){var participantes=modvotacao.find(".paredao");participantes.hover(function(){$(this).find(".btnCheck").removeClass("icon-unselected").addClass("icon-selected");},function(){$(this).find(".btnCheck").removeClass("icon-selected").addClass("icon-unselected");});}}function showCampaingButton(){if(!("permissions") in navigator||typeof navigator.permissions==="undefined"){return new Promise(function(reject,resolve){resolve({"state":Notification&&Notification.permission||"denied"});});}return navigator.permissions.query({name:"notifications"});}function checkCampaign(){var surveyButton=modulo.find(".btnSeguir");var campaign=surveyButton&&surveyButton.attr("data-campaign");if(surveyButton&&campaign){var tries=0;var init=function(){try{UOLPushWebalert.isUserSubscribed(campaign).then(function(response){toggleButtonFollow("init",response);});surveyButton.on("click",function(e){e.stopPropagation();var follow=surveyButton.hasClass("follow");var action=(follow?"":"un")+"subscribeUserTag";surveyButton.attr("disabled",true).removeClass("cor17 cor30 bg17 corw bg30").addClass("cor11 bg30");UOLPushWebalert[action]&&UOLPushWebalert[action](campaign).then(function(response){toggleButtonFollow(action,response);metricsGA({"action":"clique","category":"bloco-editorial-bbb","area":"bbb","label":(action==="subscribeUserTag"?"":"des")+"autorizar-webalert-bbb","position":"coluna-unica"});});});}catch(e){window.setTimeout(function(){if(tries<30){init();}tries++;},1000);}};showCampaingButton().then(function(response){if(response&&response.state!=="denied"){init();surveyButton.removeClass("hide");}});}}function toggleButtonFollow(action,data){DConsole.log("retorno Seguir BBB",action,data);var surveyButton=modulo.find(".btnSeguir");var actualState